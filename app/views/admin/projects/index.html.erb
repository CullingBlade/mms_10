<h1><%= t "project.list" %></h1>

<%= link_to t("project.new"), new_admin_project_path,
  class: "glyphicon glyphicon-plus" %>
<%= will_paginate %>
<div class="table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th><%= t :name %></th>
        <th><%= t :time %></th>
        <th><%= t :tool %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @projects %>
    </tbody>
  </table>
</div>
<%= will_paginate %>
