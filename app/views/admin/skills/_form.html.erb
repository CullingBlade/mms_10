<%= form_for [:admin, @skill] do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <div class="fields">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit t("submit"), class: "btn btn-primary" %>
  </div>
<% end %>
